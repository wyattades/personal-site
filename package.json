{
  "name": "personal-site",
  "private": true,
  "version": "2.0.0",
  "description": "The portfolio website of Wyatt Ades",
  "scripts": {
    "lint": "eslint src",
    "build-resume": "http-server dist -p 8088 & google-chrome --headless --disable-gpu --print-to-pdf='./dist/resume.pdf' http://localhost:8088/resume.html; kill $!",
    "postinstall": "node ./utils/treeShakeThreeJs.js",
    "dev": "NODE_ENV=development webpack-dev-server",
    "build": "NODE_ENV=production webpack",
    "build-deploy": "npm run build && npm run deploy",
    "deploy": "aws s3 rm s3://wyattades.com --recursive --include \"*.js\" --include \"*.css\" && aws s3 sync dist s3://wyattades.com/ && cf-invalidate --wait -- E1MHDHVPBNLO7M index.html"
  },
  "dependencies": {
    "animated": "^0.2.2",
    "font-awesome": "^4.7.0",
    "java-to-javascript": "^0.1.15",
    "p5": "^0.8.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.0.1",
    "three": "^0.104.0"
  },
  "devDependencies": {
    "webpack-boiler": "^0.4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wyattades/personal-site"
  }
}
