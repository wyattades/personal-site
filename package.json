{
  "name": "personal-site",
  "private": true,
  "version": "2.0.0",
  "description": "The portfolio website of Wyatt Ades",
  "scripts": {
    "lint": "eslint src",
    "get-pdf": "google-chrome --headless --disable-gpu --print-to-pdf='./src/static/resume.pdf' http://localhost:8080/resume.html",
    "postinstall": "node ./utils/treeShakeThreeJs.js",
    "dev": "NODE_ENV=development webpack-dev-server",
    "build": "NODE_ENV=production webpack",
    "build-deploy": "npm run build && npm run deploy",
    "deploy": "aws s3 rm s3://wyattades.com --recursive --include \"*.js\" --include \"*.css\" && aws s3 sync dist s3://wyattades.com/ && cf-invalidate --wait -- E1MHDHVPBNLO7M index.html"
  },
  "dependencies": {
    "animated": "^0.2.2",
    "font-awesome": "^4.7.0",
    "java-to-javascript": "^0.1.10",
    "p5": "^0.7.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-hot-loader": "^4.3.11",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.5.0",
    "three": "^0.95.0"
  },
  "devDependencies": {
    "webpack-boiler": "^0.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wyattades/personal-site"
  }
}
