{
  "name": "personal-site",
  "version": "2.0.0",
  "description": "The portfolio website of Wyatt Ades",
  "scripts": {
    "lint": "eslint .",
    "dev": "cross-env NODE_ENV=development node dev-server",
    "build": "cross-env NODE_ENV=production webpack --profile --colors",
    "build-images": "imagemin src/pre-images/* --out-dir=src/images",
    "build-deploy": "npm run build && npm run deploy",
    "deploy": "aws s3 sync dist s3://wyattades.com/ && cf-invalidate --wait -- E1MHDHVPBNLO7M index.html"
  },
  "dependencies": {
    "animated": "^0.2.1",
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bulma": "^0.6.1",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "font-awesome": "^4.7.0",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "imagemin-cli": "^3.0.0",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-loader": "^2.0.9",
    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.2.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "three": "^0.88.0",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "devDependencies": {
    "eslint": "^4.13.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "^7.5.1"
