{
  "extends": ["eslint:recommended", "next"],
  "env": {
    "node": true,
    "es2021": true,
    "browser": true
  },
  "rules": {
    "react/display-name": 0,
    "no-empty": 0,
    "no-unused-vars": [
      1,
      { "args": "all", "argsIgnorePattern": "^_", "varsIgnorePattern": "^_" }
    ]
  }
}
